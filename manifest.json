{
	"name": "GPC exercise your right",
	"version": "1.0.1",
	"manifest_version": 2,
	"description": "Enable Global Privacy Control on Google Chrome (non-official).",
	"icons": {
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [{
		"run_at": "document_start",
		"js": ["document_start.js"],
		"matches": ["<all_urls>"]
	}],
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	]
}